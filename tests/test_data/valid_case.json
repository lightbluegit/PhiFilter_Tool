      {
          "test_id": "bpm_tc011",
          "input": "15~",
          "expected": "error",
          "description": "no data after ~"
      },
      {
          "test_id": "bpm_tc012",
          "input": "0015~0016",
          "expected": "15.0~16.0",
          "description": "prefix 0(range)"
      },
      {
          "test_id": "bpm_tc013",
          "input": "0015",
          "expected": "15.0",
          "description": "prefix 0(singal)"
      }
  ],

